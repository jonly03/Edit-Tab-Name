{
  "manifest_version": 3,
  "name": "Edit and Highlight Tab",
  "version": "1.1",
  "description": "Right-click on a tab to edit its name or highlight it with a color.",
  "permissions": ["contextMenus", "scripting", "activeTab", "tabGroups"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
